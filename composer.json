{
    "name": "hankz/laravel-system-settings",
    "description": "A Laravel package designed for managing system configuration values in database.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "settings",
        "configuration",
        "database"
    ],
    "authors": [
        {
            "name": "hank",
            "email": "hankz1108@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "laravel/framework": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Hankz\\LaravelSystemSettings\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hankz\\LaravelSystemSettings\\SystemSettingProvider"
            ]
        }
    }
}